@model List<ContactModel>

<table class="table table-dark table-striped" id="table-contacts-user">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col" style="text-align: center;">Name</th>
            <th scope="col" style="text-align: center;">Email</th>
            <th scope="col" style="text-align: center;">Phone</th>
        </tr>
    </thead>
    <tbody>
        @if(Model != null && Model.Any())
        {
            @foreach (var contact in Model)
            {
                <tr>
                    <th scope="row">@contact.Id</th>
                    <td style="text-align: center;">@contact.Name</td>
                    <td style="text-align: center;">@contact.Email</td>
                    <td style="text-align: center;">@contact.Phone</td>
                </tr>
            }
        }       
    </tbody>
</table>

